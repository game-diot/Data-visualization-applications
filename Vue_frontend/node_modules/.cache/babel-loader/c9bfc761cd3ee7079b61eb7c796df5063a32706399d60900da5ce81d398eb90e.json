ta-theme', 'dark'\r\n\r\n         )},\r\n        openRegist(){\r\n                this.register = true\r\n\r\n         \r\n\r\n        },\r\n\r\n        //Êèê‰∫§Ê®°Âùó \r\n         handleLogin(){\r\n                //  let json ={ ...this.LoginForm}\r\n                //  let _this = this;\r\n                //  _this.axios.post(`${_this.baseurl}/user/userLogin`,json, {\r\n                //     headers:{\r\n                //        \"Content-Type\":\"application/json\",\r\n                //        \"Authorization\":''\r\n                //     }\r\n                //  }).then(function(res){\r\n                //    if(res.data.code == 1){\r\n                //       localStorage.setItem('token',res.data.token);\r\n                //       console.log(localStorage.getItem.token)//Ë∞ÉËØïÁî®\r\n                //       setTimeout(() =>{_this.$router.replace({\r\n                //          path:'/index'\r\n                //       })},100)\r\n                //    }else{\r\n                //       alert(res.data.msg);\r\n                //    }\r\n                //  })\r\n         },\r\n        handleRegist(){\r\n          \r\n                //  let json ={ ...this.LoginForm}\r\n                //  let _this = this;\r\n                //  _this.axios.post(`${_this.baseurl}/user/userRegist`,json, {\r\n                //     headers:{\r\n                //        \"Content-Type\":\"application/json\",\r\n                //        \"Authorization\":''\r\n                //     }\r\n                //  }).then(function(res){\r\n                //    if(res.data.code == 1){\r\n                //       localStorage.setItem('token',res.data.token);\r\n                //       console.log(localStorage.getItem.token)//Ë∞ÉËØïÁî®\r\n                //       setTimeout(() =>{_this.$router.replace({\r\n                //          path:'/index'\r\n                //       })},100)\r\n                //    }else{\r\n                //       alert(res.data.msg);\r\n             